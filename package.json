{
  "name": "silverstripe-keepclassy",
  "description": "Keep your classes classy",
  "main": "./client/src/bundles/bundle.js",
  "author": "Robbie Averill",
  "engines": {
    "node": ">=6.x"
  },
  "scripts": {
    "build": "yarn && NODE_ENV=production webpack -p --bail --progress",
