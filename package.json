    "jquery": "^3.1.1"
  },
  "devDependencies": {
    "@silverstripe/eslint-config": "^0.0.4",
    "@silverstripe/webpack-config": "^0.8.0",
    "webpack": "^2"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
